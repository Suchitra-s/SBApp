                <Button x:Name="S_Button" Text="S" WidthRequest="60" CornerRadius="40" HeightRequest="60" Clicked="S_Clicked" BackgroundColor="White"/>
                <Button x:Name="T_Button" Text="T" WidthRequest="60" CornerRadius="40" HeightRequest="60" Clicked="T_Clicked" BackgroundColor="White"/>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Button x:Name="U_Button" Text="U" WidthRequest="60" CornerRadius="40" HeightRequest="60" Clicked="U_Clicked" BackgroundColor="White"/>
