                <Button x:Name="L_Button" Text="L" WidthRequest="60" CornerRadius="40" HeightRequest="60" Clicked="L_Clicked" BackgroundColor="White"/>
            </StackLayout>
            <StackLayout Orientation="Horizontal">
                <Button x:Name="M_Button" Text="M" WidthRequest="60" CornerRadius="40" HeightRequest="60" Clicked="M_Clicked" BackgroundColor="White"/>
